{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": false,
    "arrow-parens": false,
    "ordered-imports": false,
    "semicolon": false,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "no-console": [true, "log"],
    "object-literal-sort-keys": [false],
    "object-literal-key-quotes": [true, "consistent-as-needed"]
  },
  "rulesDirectory": []
}
