{
  "name": "rve",
  "version": "0.0.9",
  "description": "React mixed Vue 3.0 API with type safety",
  "main": "./dist/rve.min.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "yarn run tslint && webpack --mode development",
    "dev:watch": "nodemon --exec 'yarn run dev || exit 1'",
    "tslint": "tslint --project .",
    "build": "yarn run tslint && webpack --mode production",
    "clean": "rm -rf dist",
    "demo": "nodemon --exec 'cd demo && yarn && yarn run dev'",
    "pub": "yarn run build && yarn publish --registry http://registry.npmjs.org"
  },
  "author": "yetone <yetoneful@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.18",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/source-map": "0.5.2",
    "@types/webpack": "^4.4.21",
    "jest": "^23.6.0",
    "nodemon": "^1.17.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@types/object-path": "^0.11.0",
    "object-path": "^0.11.4",
    "object-path-immutable": "^3.1.1"
  }
}
