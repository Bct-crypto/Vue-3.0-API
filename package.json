{
  "name": "rve",
  "version": "0.1.0",
  "description": "React mixed Vue 3.0 API with type safety",
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "jsnext:main": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "tslint": "tslint --project .",
    "build": "yarn run tslint && rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "clean": "rm -rf dist",
    "demo": "nodemon --exec 'cd demo && yarn && yarn run dev'",
    "deploy": "yarn run prepare && yarn publish --registry http://registry.npmjs.org"
  },
  "author": "yetone <yetoneful@gmail.com>",
  "license": "MIT",
  "repository": "yetone/rve",
  "devDependencies": {
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.18",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/rollup": "^0.54.0",
    "@types/source-map": "0.5.2",
    "jest": "^23.6.0",
    "nodemon": "^1.17.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.22.1",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@types/object-path": "^0.11.0",
    "object-path": "^0.11.4",
    "object-path-immutable": "^3.1.1"
  }
}
