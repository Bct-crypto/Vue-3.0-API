{
  "name": "rve",
  "version": "0.1.2",
  "description": "React mixed Vue 3.0 API with type safety",
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "jsnext:main": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx}'",
    "build": "yarn run lint && rollup -c",
    "start": "rollup -c -w",
    "clean": "rm -rf dist",
    "demo": "nodemon --exec 'cd demo && yarn && yarn run dev'",
    "deploy": "yarn run build && yarn publish --registry http://registry.npmjs.org",
    "test": "jest"
  },
  "author": "yetone <yetoneful@gmail.com>",
  "license": "MIT",
  "repository": "yetone/rve",
  "devDependencies": {
    "@testing-library/react": "^9.1.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^10.12.18",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/rollup": "^0.54.0",
    "@types/source-map": "0.5.2",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "jest": "^24.9.0",
    "nodemon": "^1.17.5",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.22.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "typescript": "3.5.3"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@types/object-path": "^0.11.0",
    "object-path": "^0.11.4",
    "object-path-immutable": "^3.1.1"
  }
}
